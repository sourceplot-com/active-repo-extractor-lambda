{
  "name": "sourceplot-data-extractor-lambda",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "zip": "zip -r function.zip dist/ node_modules/",
    "package": "NODE_ENV=production npm run clean && npm ci && npm run build && npm ci --omit=dev --ignore-scripts && npm run zip",
    "prepare": "husky"
  },
  "dependencies": {
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1692.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/node": "^22.13.4",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
